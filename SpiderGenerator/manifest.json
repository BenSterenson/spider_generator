{
  "description": "Sixgill",
  "manifest_version": 2,
  "name": "Sixgill",
  "version": "1.0",
  "author": "Ben Sterenson",

  "browser_action": {
    "default_icon": "devtools/assets/img/star.png"
  },

  "icons": {
    "48": "devtools/assets/img/star.png"
  },
  
  "background": {
    "scripts": ["background_scripts/background.js"]
  },
  
  "permissions": [
      "<all_urls>"
  ],
  "devtools_page": "devtools/devtools-page.html"
}